[
  {% for model in site.data.models %}
    {
      "title" : "{{ model.name | escape }}",
      "description" : "{{ model.description | strip_newlines | strip_html | strip | escape }}",
      "url" : "{{ model.link }}",
      "image" : "{{ model.image }}",
      "content" : "{{ model.description | truncate: '300' | strip_html | strip_newlines | strip | escape }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
